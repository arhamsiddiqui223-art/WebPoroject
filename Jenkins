pipeline {
    agent any
    stages {
        stage('Checkout') {
            steps {
                git branch: 'main', url: 'https://github.com/arhamsiddiqui223-art/webproject.git', credentialsId: 'github-creds'
            }
        }
        stage('Deploy') {
            steps {
                bat 'xcopy * C:\\xampp\\htdocs\\ /E /Y'
            }
        }
    }
}
